{
  "name": "wechat-ci",
  "version": "0.2.6",
  "description": "微信小程序 上传代码/预览, 命令行 上传/预览 微信小程序",
  "main": "./lib/index.js",
  "bin": {
    "wechat-ci": "lib/index.js"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "release": "npm run build && release-it",
    "build": "tsc -p tsconfig.json",
    "commit": "git-cz",
    "commit:origin": "git pull && git add . && git-cz && git push",
    "lint": "prettier --write \"**/*.{js,ts,json,vue,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaoyao-Ye/wechat-ci.git"
  },
  "keywords": [
    "wechat-upload",
    "wechat-preview"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xiaoyao-Ye/wechat-ci/issues"
  },
  "homepage": "https://github.com/xiaoyao-Ye/wechat-ci#readme",
  "devDependencies": {
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/inquirer": "^8.2.1",
    "@types/node": "^18.6.4",
    "git-cz": "^4.9.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "release-it": "^15.2.0",
    "simple-git-hooks": "^2.8.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,vue}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "inquirer": "8.0.0",
    "jiti": "^1.14.0",
    "miniprogram-ci": "^1.8.35",
    "semver": "^7.3.7"
  }
}
