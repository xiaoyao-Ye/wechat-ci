{
  "name": "wechat-ci",
  "version": "1.0.0",
  "description": "微信小程序持续构建, 持续交付, 持续部署",
  "main": "src/index.js",
  "bin": {
    "wechat-ci": "src/index.js"
  },
  "scripts": {
    "commit": "git-cz",
    "commit:origin": "git pull && git add . && git-cz && git push",
    "lint": "prettier --write \"**/*.{js,ts,json,vue,md}\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xiaoyao-Ye/wechat-ci.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xiaoyao-Ye/wechat-ci/issues"
  },
  "homepage": "https://github.com/xiaoyao-Ye/wechat-ci#readme",
  "devDependencies": {
    "git-cz": "^4.9.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "simple-git-hooks": "^2.8.0"
  },
  "simple-git-hooks": {
    "pre-commit": "npm run lint-staged"
  },
  "lint-staged": {
    "*.{js,ts,json,md,vue}": [
      "prettier --write"
    ]
  }
}
